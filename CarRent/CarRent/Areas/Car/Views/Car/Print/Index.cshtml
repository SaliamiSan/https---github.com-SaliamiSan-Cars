@model IEnumerable<Infrastruction.DomainObjects.Car>

@{
    ViewBag.Title = "Index";
    Layout = null;
}

<h2>Print Version</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CarName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.Partial("CarStatus", item)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
            </td>
        </tr>
    }
</table>
